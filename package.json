{
  "name": "react-shadcn-theme",
  "version": "1.0.4",
  "type": "module",
  "description": "A React theme provider for shadcn/ui",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "styles.css"
  ],
  "scripts": {
    "build": "rollup -c && cp src/styles.css styles.css",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "theme",
    "shadcn",
    "ui"
  ],
  "author": "Mayank Singh Bhetwal",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/react": "^18.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-css-only": "^4.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0"
  }
}
